{
  "name": "react-native-identify-sdk-view-example",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "cd android && ./gradlew assembleDebug --no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a",
    "build:ios": "cd ios && xcodebuild -workspace IdentifySdkViewExample.xcworkspace -scheme IdentifySdkViewExample -configuration Debug -sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "pod-install": "^0.1.0",
    "react-native-identify-sdk": "git+https://github.com/canibal2/react-native-identify-sdk.git"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-document-scanner-plugin": "*",
    "react-native-dotenv": "*",
    "react-native-fast-tflite": "*",
    "react-native-fs": "*",
    "react-native-gesture-handler": "*",
    "react-native-gradle-plugin": "*",
    "react-native-localize": "*",
    "react-native-nfc-manager": "*",
    "react-native-nfc-passport-reader": "*",
    "react-native-safe-area-context": "*",
    "react-native-screens": "*",
    "react-native-signature-canvas": "*",
    "react-native-svg": "*",
    "react-native-vision-camera": "*",
    "react-native-volume-manager": "*",
    "react-native-webrtc": "*",
    "react-native-webview": "*",
    "react-native-worklets-core": "*"
  },
  "engines": {
    "node": ">=18"
  }
}
